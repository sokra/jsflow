var $ = require("jquery");
var jsflow = require("./lib/jsflow");
var hljs = require("highlight.js");
$() ==> {
	function compile() {
		$("#input").val() ->
			jsflow.compile(#) ->
			$("#output").text(#) ->
			hljs.highlightBlock(#[0]);
		~> # && #.stack || # ->
			$("#output").text(#);
	}
	$("#input").on("change") ==> compile();
	$("#input").on("keyup") ==> compile();
	compile();
	require("./package.json").version ->
		$("#headline").text() + " version " + # ->
		$("#headline").text(#);
}